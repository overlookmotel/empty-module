{
  "name": "{{name}}",
  "version": "0.0.0",
  "description": "{{description}}",
  "main": "index.js",
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/{{name}}.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/{{name}}/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@overlookmotel/eslint-config": "^2.1.0",
    "@overlookmotel/eslint-config-tests": "^1.0.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "keywords": [
    {{keywords}}
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint '**/*.js' --ext .js",
    "test-main": "mocha --check-leaks --colors -t 1000 -R spec 'test/**/*.test.js'",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true istanbul cover _mocha --report lcovonly -- -t 1000 -R spec 'test/**/*.test.js'",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=8"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
