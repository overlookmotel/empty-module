{
  "name": "{{name}}",
  "version": "0.0.0",
  "description": "{{description}}",
  "main": "index.js",
  "files": [
    "lib/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/{{name}}.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/{{name}}/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@overlookmotel/eslint-config": "^9.0.6",
    "@overlookmotel/eslint-config-jest": "^5.0.1",
    "@overlookmotel/eslint-config-node": "^3.0.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^35.4.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5"
  },
  "keywords": [
    "{{keywords}}"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test-main": "jest",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true jest --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=12"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
